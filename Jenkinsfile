pipeline{
    agent  any 
    stages{
        stage('Checkout'){
            steps{
               sh 'echo "Checkout ==>>"'
            }
        }
        stage('Build'){
            steps{
                sh 'echo "Building"'
            }
        }

        stage('Deploy'){
            steps{
                sh 'echo "Deploy"'
            }
        }
    }
}